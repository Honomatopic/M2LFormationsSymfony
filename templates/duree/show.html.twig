{% extends 'base.html.twig' %}

{% block body %}
    <h1>La durée</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ duree.id }}</td>
            </tr>
            <tr>
                <th>Datedebut</th>
                <td>{{ duree.datedebut ? duree.datedebut|date('d/m/Y') : '' }}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{{ duree.datefin ? duree.datefin|date('d/m/Y') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('duree_index') }}">&#8617;&#65039;Retour à la liste des durées</a>

    <a href="{{ path('duree_edit', {'id': duree.id}) }}">&#128395;&#65039;Modifier</a>

    {{ include('duree/_delete_form.html.twig') }}
{% endblock %}
